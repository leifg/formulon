{
  "name": "formulon",
  "version": "0.0.0-development",
  "description": "Salesforce Formula Parser",
  "main": "lib/formulon.js",
  "engines": {
    "node": ">=11.0.0"
  },
  "scripts": {
    "build": "yarn compile:parser && yarn rollup -c",
    "compile:parser": "node .scripts/make-peggy",
    "test": "mocha --require @babel/register --recursive --grep @integration --invert",
    "test:integration": "mocha --require @babel/register --recursive --grep @integration",
    "lint": "eslint --ignore-path .gitignore .",
    "commitlint": "commitlint",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leifg/formulon.git"
  },
  "keywords": [
    "parser",
    "salesforce",
    "formula"
  ],
  "author": "Leif Gensert",
  "license": "MIT",
  "dependencies": {
    "decimal.js": "^10.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "@commitlint/config-conventional": "^17.0.0",
    "@types/node": "^20.1.4",
    "chai": "^4.2.0",
    "commitlint": "^17.0.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.20.2",
    "glob": "^10.1.0",
    "mocha": "^10.0.0",
    "peggy": "^3.0.0",
    "rollup": "^2.26.5",
    "rollup-plugin-terser": "^7.0.0",
    "semantic-release": "^21.0.0"
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}
